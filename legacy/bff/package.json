  "description": "Mylo - BFF",
  "main": "index.js",
  "scripts": {
    "start": "node build/src/app/web/index.js",
    "dev": "nodemon --legacy-watch --watch 'src/**/*' -e ts --exec ts-node src/app/web/index.ts",
    "build": "npx tsc",
    "copy_assets": "cp -r src/app/web/shared/views build/src/app/web/shared/views && cp -r src/app/web/shared/public build/src/app/web/shared/public && cp -r src/app/web/partner/views build/src/app/web/partner/views && cp -r src/app/web/partner/public build/src/app/web/partner/public && cp -r src/app/web/static/data build/src/app/web/static && cp -r src/app/web/back-office/views build/src/app/web/back-office/views && cp -r src/app/web/back-office/public build/src/app/web/back-office/public",
    "eslint": "eslint --max-warnings=0",
    "lint:js": "eslint --ext .ts src/** --max-warnings=0",
    "lint:js:fix": "eslint --ext .ts src/** --max-warnings=0 --fix",
    "lint:ejs": "npx ejslint **/*.ejs",
    "shell": "ts-node",
    "test:cov": "jest --coverage",
    "test": "mocha -r ts-node/register test/**/*.test.ts --timeout 60000 --exit"
  },
  "author": "",
  "license": "ISC",
